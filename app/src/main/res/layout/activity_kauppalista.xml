<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfiiliActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#E81D62"
        android:layout_marginHorizontal="2dp">

        <com.toptoche.searchablespinnerlibrary.SearchableSpinner android:id="@+id/spinner_search" android:layout_width="wrap_content" android:layout_height="50dp" />

        <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/spinner_search" android:background="@drawable/taustak">
        </LinearLayout>

    </RelativeLayout>




</androidx.constraintlayout.widget.ConstraintLayout>

